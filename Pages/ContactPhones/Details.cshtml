@page "{id:int}"
@model McpWebApp.Pages.ContactPhones.DetailsModel
@{
    ViewData["Title"] = "Contact Phone Details";
}
<h1>Contact Phone Details</h1>
<div>
    <h4>Contact Phone</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Phone Type</dt>
        <dd class="col-sm-10">@Model.ContactPhone.PhoneType</dd>
        <dt class="col-sm-2">Phone Number</dt>
        <dd class="col-sm-10">@Model.ContactPhone.PhoneNumber</dd>
        <dt class="col-sm-2">Contact Id</dt>
        <dd class="col-sm-10">@Model.ContactPhone.ContactId</dd>
    </dl>
</div>
<p>
    <a asp-page="Edit" asp-route-id="@Model.ContactPhone.Id">Edit</a> |
    <a asp-page="Index">Back to List</a>
</p>
